<h1>New attendee</h1>

<% form_for([@event, @attendee]) do |f| %>
  <%= f.error_messages %>
  <p>    
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </p>
  <p>
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>
<%= link_to 'Back', event_attendees_path(@event) %> 
